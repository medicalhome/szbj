<?xml version="1.0" encoding="UTF-8"?>
<Head>
	<!--字段分隔符-->
	<FieldSeparator>字段分隔符</FieldSeparator>
	<!--代码字符-->
	<EncodingCharacters>代码字符</EncodingCharacters>	
   <!--发送方应用程序-->
   <SendApplication>发送方应用程序</SendApplication>
   <!--接收方应用程序-->
   <ReceivingApplication>接收方应用程序</ReceivingApplication>
  <!--消息日期-->
   <TimeOfMessage>消息日期</TimeOfMessage> 
   <!--消息类型-->
   <MessageType>消息类型</MessageType>
   <!--消息ID-->
   <MessageControlId></MessageControlId>
   <!--处理ID-->
   <ProcessingId>处理ID</ProcessingId>
   <!--版本ID-->
   <VersionId>版本ID</VersionId>
    <!--消息接收确认类型-->
   <AcceptAckType>消息接收确认类型</AcceptAxkType>
    <!--消息接收确认类型-->
   <ApplicationAckType>应用程序确认类型</ApplicationAckType>   
    <!--国家代码-->
   <CountryCode>国家代码</CountryCode>   	
</Head>
<Body>
	<!-- 检查申请单信息-->
	<Patient>
		<!-- 病人ID  -->
		<PatientId>病人ID</PatientId>
		<IdList>
			<Item>
				<!--患者标识id-->
				<Id>患者标识id</Id>
				<!--患者标识类型  InpatientNO:住院号 OutpatientNo：门诊号 MedicareNo：医保卡号 PatientNo:-病历编号-->
				<IdTypeCode>患者标识类型</IdTypeCode>
			</Item>
		</IdList>
		<!-- 姓名  -->
		<PatientName>姓名</PatientName>
		<!-- 医院Id  -->
		<HospitalId>医院Id</HospitalId>
		<!-- 性别  -->
		<Sex>性别</Sex>
		<!-- 出生日期  -->
		<Birthday>年龄</Birthday>
	</Patient>
	<Visit>
		<!-- 就诊类型  -->
		<MedicalType>就诊类型</MedicalType>
		<!-- 就诊流水号  -->
		<VisitNo>就诊流水号</VisitNo>
		<!-- 就诊次数  -->
		<VisitTimes>就诊次数</VisitTimes>
		<!-- 病区编码  -->
		<WardCode>病区编码</WardCode>
		<!-- 病房号(病区)  -->
		<Ward>病房号(病区)</Ward>
		<!-- 床号  -->
		<BedNo>床号</BedNo>
		<!--就诊科室编码 -->
		<DeptCode>就诊科室编码 </DeptCode>
		<!--就诊科室名称 -->
		<DeptName>就诊科室名称 </DeptName>	
	<Visit>
	<Application>
		<CommonOder>
			<!-- 申请控制 取值范围( ) -->
			<OrderControl>申请控制</OrderControl>
			<!-- 申请单号  -->
			<ApplyNo>申请单号</ApplyNo>
			<!-- 医嘱编号   -->
			<AdviceNo>医嘱编号 </AdviceNo>
			<!-- 检查单类型  0:正常 1：加急-->
			<ApplicationType>检查单类型</ApplicationType>
			<!-- 申请时间  -->
			<AdviceTime>申请时间</AdviceTime>
			<!-- 申请会诊时间  -->
			<ConsultationTime>申请会诊时间</ConsultationTime>
			<!-- 申请医生编码  -->
			<AdviceDoctorCode>申请医生</AdviceDoctorCode>		
			<!-- 申请医生  -->
			<AdviceDoctorName>申请医生</AdviceDoctorName>
				<!-- 申请科室编码  -->
			<AdviceDeptCode>申请科室编码</AdviceDeptCode>	
			<!-- 申请科室  -->
			<AdviceDeptName>申请科室</AdviceDeptName>
			<!-- 检查单状态  -->
			<State>检查单状态</State>
		</CommonOder>
		<ObservertionRequest>
			<!-- 检查项目类型  -->
			<CheckItemType>检查项目类型</CheckItemType>
			<!-- 检查项目代码  -->
			<CheckItemCode>检查项目代码</CheckItemCode>
			<!-- 检查项目名称  -->
			<CheckItemName>检查项目名称</CheckItemName>
			<!-- 执行科室编码  -->
			<ExecuteDeptCode>执行科室编码</ExecuteDeptCode>
			<!-- 执行科室名称  -->
			<ExecuteDeptName>执行科室名称</ExecuteDeptName>
			<!-- 备注  -->
			<Remark>备注</Remark>
		<ObservertionRequest>
		<ObsResult>
			<!--identifier:
			SIGN:临床症状及体征  HISTORY:既往病史
			RIS:检查结果  LIS:检验结果  DRUG:用药情况
			REMARK:备注 DIAG:诊断  SITE：部位描述-->
			<!--观察标识符-->
			<ObsIdentifier>观察标识符</ObsIdentifier>
			<!--值类型 ST-->
			<ValueType></ValueType>
			<!--观察值-->
			<ObservationValue>观察值</ObservationValue>
		</ObsResult>
	</Application>
</Body>
